# Docker aliases
alias drun='docker ps'
alias dall='docker ps -a'
alias drm='docker container prune -f'
alias drmi='docker image prune -f'
alias dstop='docker stop $(docker ps -q)'
alias drm-all='docker rm -f $(docker ps -aq)'
alias dbuild='docker build -t'
alias dbuildx='docker buildx build --platform linux/amd64 --load'
alias dimages='docker images'
alias drmi-id='docker rmi'
alias dlogs='docker logs -f'
alias dexec='docker exec -it'
alias dquick='docker run --rm -it'
alias dclean='docker system prune -a -f --volumes'
alias drn='docker network prune -f'
alias dcu='docker compose up -d'
alias dcd='docker compose down'
alias dclogs='docker compose logs -f'
